{
	"name": "mkfizi.github.io",
	"version": "1.0.0",
	"description": "Portfolio Website",
	"keywords": [
		"Portfolio",
		"Website",
		"Portfolio Website"
	],
	"author": "mkfizi93@gmail.com",
	"license": "MIT",
	"scripts": {
		"build": "npx tailwindcss -i ./src/css/input.css -o ./src/css/style.css && minify ./src/js/script.js > ./src/js/script.min.js && minify ./src/css/style.css > ./src/css/style.min.css",
		"build:prod": "npx tailwindcss -i ./src/css/input.css -o ./src/css/style.min.css --minify",
		"watch": "npx tailwindcss -i ./src/css/input.css -o ./src/css/style.css --watch",
		"watch:prod": "npx tailwindcss -i ./src/css/input.css -o ./src/css/style.min.css --watch --minify"
	},
	"devDependencies": {
		"tailwindcss": "^3.1.3"
	},
	"dependencies": {
		"minify": "^9.1.0"
	}
}
