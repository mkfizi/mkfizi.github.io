	"scripts": {
		"build": "npx tailwindcss -i ./src/css/tailwind.css -o ./src/css/app.css ",
		"watch": "npx tailwindcss -i ./src/css/tailwind.css -o ./src/css/app.css --watch",
		"minify": "npm run build && npx tailwindcss -i ./src/css/tailwind.css -o ./src/css/app.min.css --minify & minify ./src/js/app.js > ./src/js/app.min.js"
	},
	"devDependencies": {
		"tailwindcss": "^3.1.3"
	},
	"dependencies": {
		"minify": "^9.1.0"
	}
}
